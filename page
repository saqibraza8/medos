                        </div>

                        <script>

                            ons.getScriptPage().onInit = function () {

                                this.querySelector('ons-toolbar div.center').textContent = this.data.title;

                                const carouselCueCard = document.getElementById('carouselCueCard');

                                carouselCueCard.addEventListener('postchange', function () {

                                    var index = carouselCueCard.getActiveIndex();

                                    const dots = document.querySelectorAll('.dot');

                                    for (dot of dots) {

                                        dot.innerHTML = dot.id === 'dot' + index ? '&#9679;' : '&#9675;';

                                    }

                                });

                                window.fn.swipe = function (target) {

                                    carouselCueCard.setActiveIndex(Number(target.id.slice(-1)));

                                }

                            }

                        </script>

                        <style>

                            .carousel-item {
                                display: flex;
                                justify-content: space-around;
