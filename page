                <template id="cuecards.html">

                    <ons-page id="subcue">
                        <ons-toolbar>

                            <div class="left">

                                <ons-back-button>Cue Cards</ons-back-button>

                            </div>

                            <div class="center"></div>

                        </ons-toolbar>

                        <div class="content">
                            <ons-carousel id="carouselCueCard" fullscreen swipeable auto-scroll overscrollable initial-index="0" style="height:90%">



                                <ons-carousel-item class="carousel-item" style="background-color:white" ng-repeat="item in cueCardArray track by $index">

                                    <ons-card style="height:100%;width:100%">
                                        <p><b>Question:</b></p>
                                        <p><b>{{item.qus}}</b></p>
